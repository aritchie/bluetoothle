        <projectUrl>https://github.com/aritchie/bluetoothle</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <tags>ios android uwp windows tvos mac macos xamarin rx reactive ble bluetooth bluetoothle gattserver advertising</tags>
